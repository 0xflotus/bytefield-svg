{
  "name": "bytefield-svg",
  "version": "1.2.0",
  "description": "Generate byte field diagrams in SVG format from a Clojure/EDN DSL",
  "main": "lib.js",
  "bin": "cli.js",
  "dependencies": {
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0"
  },
  "devDependencies": {
    "@antora/cli": "^2.2.0",
    "@djencks/content-aggregator": "https://gitlab.com/djencks/antora/-/jobs/artifacts/djencks-issue-377-events/raw/packages/content-aggregator/djencks-content-aggregator-v2.3.0-beta.1.tgz?job=bundle-stable",
    "@djencks/playbook-builder": "https://gitlab.com/djencks/antora/-/jobs/artifacts/djencks-issue-377-events/raw/packages/playbook-builder/djencks-playbook-builder-v2.3.0-beta.1.tgz?job=bundle-stable",
    "@djencks/site-generator-default": "https://gitlab.com/djencks/antora/-/jobs/artifacts/djencks-issue-377-events/raw/packages/site-generator-default/djencks-site-generator-default-v2.3.0-beta.1.tgz?job=bundle-stable",
    "antora-generic-svg-extension-plugin": "https://gitlab.com/djencks/antora-generic-svg-extension-plugin/-/jobs/artifacts/master/raw/antora-generic-svg-extension-plugin-v0.0.1.tgz?job=bundle-stable",
    "antora-lunr": "https://github.com/djencks/antora-lunr#plugin-377",
    "asciidoctor-mathjax": "0.0.4",
    "shadow-cljs": "^2.8.94",
    "source-map-support": "^0.5.16"
  },
  "scripts": {
    "build": "shadow-cljs compile lib",
    "release": "shadow-cljs release lib",
